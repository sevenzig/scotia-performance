{
	"name": "scotia-performance",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "bun x vite dev",
		"build": "bun x vite build",
		"preview": "bun x vite preview",
		"prepare": "bun x svelte-kit sync",
		"test:unit": "bun x vitest",
		"test": "bun test",
		"lint": "bun x eslint . && bun x prettier --check .",
		"format": "bun x prettier --write .",
		"check": "bun x svelte-check --tsconfig ./jsconfig.json --threshold error",
		"check:watch": "bun x svelte-check --tsconfig ./jsconfig.json --watch --threshold error",
		"optimize:images": "bun run scripts/optimize-images.js",
		"analyze": "bun x vite build --mode analyze",
		"analyze:performance": "bun run scripts/analyze-performance.js",
		"perf": "bun run analyze:performance",
		"postbuild": "bun run perf"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"@tailwindcss/vite": "^4.0.0",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.4",
		"chalk": "^5.3.0",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"jsdom": "^26.0.0",
		"nanospinner": "^1.2.2",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"rollup-plugin-visualizer": "^5.12.0",
		"sass": "^1.69.0",
		"sharp": "^0.33.2",
		"svelte": "^5.0.0",
		"svelte-check": "^3.6.8",
		"svelte-preprocess": "^5.1.0",
		"tailwindcss": "^4.0.0",
		"terser": "^5.39.0",
		"vite": "^6.2.6",
		"vite-plugin-imagemin": "^0.6.1",
		"vitest": "^3.0.0"
	},
	"dependencies": {},
	"trustedDependencies": [
		"sharp",
		"@rollup/rollup-linux-x64-gnu",
		"lightningcss"
	]
}
